{
  "name": "@jimmy-nitron/fastify-kick-start",
  "version": "1.1.0",
  "description": "A comprehensive, reusable Fastify library with decorators, plugins, and Swagger integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./decorators": {
      "import": "./dist/decorators/index.mjs",
      "require": "./dist/decorators/index.js",
      "types": "./dist/decorators/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir dist-esm && node scripts/rename-esm.js",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist dist-esm",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"examples/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\" \"examples/**/*.ts\" \"*.{js,json,md}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "prepublishOnly": "npm run quality && npm run test && npm run build",
    "example:basic": "ts-node examples/basic-server.ts",
    "example:advanced": "ts-node examples/advanced-server.ts"
  },
  "keywords": [
    "fastify",
    "typescript",
    "decorators",
    "swagger",
    "openapi",
    "rest-api",
    "web-framework",
    "microservices",
    "dependency-injection"
  ],
  "author": "B Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedgamalelazab/fastify-kick-start"
  },
  "bugs": {
    "url": "https://github.com/ahmedgamalelazab/fastify-kick-start/issues"
  },
  "homepage": "https://github.com/ahmedgamalelazab/fastify-kick-start#readme",
  "peerDependencies": {
    "fastify": "^5.0.0",
    "reflect-metadata": "^0.2.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^5.2.0",
    "@sinclair/typebox": "^0.33.15",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "fastify": "^5.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
